{
    "name": "spomky-labs/ip-filter-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 Ip Filter Bundle",
    "keywords": ["ip", "ipv4", "ipv6", "environment", "range", "filter", "Symfony2", "bundle"],
    "homepage": "https://github.com/Spomky/SpomkyLabsIpFilterBundle",
    "license": "MIT",
    "authors": [
        {
          "name": "Florent Morselli"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "~2.3",
        "doctrine/doctrine-bundle": "~1.0"
    },
    "require-dev": {
        "symfony/security-bundle": "~2.3",
        "symfony/finder": "~2.3",
        "symfony/class-loader": "~2.3",
        "symfony/yaml": "~2.3",
        "symfony/browser-kit": "~2.3",
        "symfony/expression-language": "~2.3",

        "doctrine/doctrine-fixtures-bundle": "*@dev",
        "vipsoft/doctrine-data-fixtures-extension": "3.*@dev",
        "behat/behat": "3.*",
        "behat/symfony2-extension": "*@dev",
        "behat/mink": "*",
        "behat/mink-extension": "*",
        "behat/mink-browserkit-driver":  "*",
        "doctrine/orm": ">=2.2,<2.5-dev"
    },
    "autoload": {
        "psr-4": { "SpomkyLabs\\IpFilterBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": {
            "SpomkyLabs\\TestIpFilterBundle\\": "Tests/TestIpFilterBundle/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
